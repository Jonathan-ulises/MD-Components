    <item
        android:id="@+id/action_favorites"
        android:title="@string/menu_favorites"
        android:icon="@drawable/ic_favorite" />

    <item
        android:id="@+id/action_profile"
        android:title="@string/menu_profile"
        android:icon="@drawable/ic_menu" />
</menu>